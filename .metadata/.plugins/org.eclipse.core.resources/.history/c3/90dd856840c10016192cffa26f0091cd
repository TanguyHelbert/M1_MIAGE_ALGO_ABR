package proj;

public class Case {
	private int min;
	private int max;
	private ArbreBinaireRecherche abr;
	
	public Case(int min, int max, ArbreBinaireRecherche abr) {
		this.min = min;
		this.max = max;
		this.abr = new ArbreBinaireRecherche();
	}

	public int getMin() {
		return min;
	}

	public void setMin(int min) {
		this.min = min;
	}

	public int getMax() {
		return max;
	}

	public void setMax(int max) {
		this.max = max;
	}

	public ArbreBinaireRecherche getAbr() {
		return abr;
	}

	public void setAbr(ArbreBinaireRecherche abr) {
		this.abr = abr;
	}
}
